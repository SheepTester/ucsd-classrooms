{
  "name": "ucsd-classrooms",
  "version": "0.1.0",
  "author": "SheepTester",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SheepTester/ucsd-classrooms.git"
  },
  "type": "module",
  "scripts": {
    "build": "cp -a static/. dist/ && esbuild --bundle src/index.tsx --loader:.webp=file --loader:.svg=file --outdir=dist --minify && find dist/ -type f | sed 's/dist\\//https:\\/\\/sheeptester.github.io\\/ucsd-classrooms\\//' > dist/sitemap.txt",
    "clean": "rm -rf dist/",
    "dev": "esbuild --bundle src/index.tsx --loader:.webp=file --loader:.svg=file --outdir=static --servedir=static --sourcemap"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.44.1"
  }
}
